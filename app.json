{
  "expo": {
    "name": "Itisha Food Courier",
    "slug": "Courier",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#635BFF"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/audio/notify.wav"]
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#635BFF"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA_ROLL",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "versionCode": 3,
      "package": "com.naivashadev.Courier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
